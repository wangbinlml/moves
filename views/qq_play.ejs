<% include partials/header %>
<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Page Content -->
    <div class="container">
        <div class="row">
            <div class="col-md-9 order-md-1">
                <section class="panel">
                    <div class="panel-body table-responsive">
                        <div id="youku_player">
                            <embed src="http://imgcache.qq.com/tencentvideo_v1/player/TencentPlayer.swf?vid=y9eypyf387g5q6n&autoplay=true&list=2&adplay=0&showcfg=1&tpid=0&pic=http://www.kuke1.com/v/shipinbg.jpg&share=1&skin=http://imgcache.qq.com/minivideo_v1/vd/res/skins/TencentPlayerMiniSkin.swf"
                                   allowFullScreen="true"
                                   quality="high" width="100%" height="400" align="middle" allowScriptAccess="always"
                                   type="application/x-shockwave-flash"></embed>

                            <object classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000"
                                    codebase="http://fpdownload.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=11,0,0,0"
                                    height="448" width="551" align="middle" onError="">
                                <param name="allowScriptAccess" value="never">
                                <param name="allowNetworking" value="internal">
                                <param name="allowFullScreen" value="true">
                                <param name="movie" value="<%= currentMoveUrls['url'] %>">
                                <embed src="<%= currentMoveUrls['url'] %>" height="448" width="551" type="application/x-shockwave-flash"
                                       pluginspage="http://www.macromedia.com/go/getflashplayer"
                                       allowscriptaccess="never" allowNetworking="internal" menu="false" loop="false"
                                       play="true" wmode="transparent"></embed>
                            </object>
                        </div>
                        <% include partials/move_detail %>
                        <% include partials/relation_list %>
                    </div>
                </section>
            </div>
            <% include partials/most_views_list %>
            <% include partials/tags_list %>
            <% include partials/area_list %>
        </div>
    </div>
</div>
<% include partials/footer %>